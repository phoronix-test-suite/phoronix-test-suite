<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="xsl/pts-exdep-viewer.xsl" ?>
<PhoronixTestSuite>
	<ExternalDependencies>
		<Information>
			<Name>NetBSD</Name>
			<PackageManager>pkg_add</PackageManager>
		</Information>
		<Package>
			<GenericName>32bit-compatibility</GenericName>
			<PackageName>suse_base suse32_base</PackageName>
			<FileCheck>/usr/pkg/emul/linux/bin/bash</FileCheck>
		</Package>
		<Package>
			<GenericName>gtk-development</GenericName>
			<PackageName>gtk2+</PackageName>
			<FileCheck>/usr/pkg/include/gtk-2.0</FileCheck>
		</Package>
		<Package>
			<GenericName>sdl-development</GenericName>
			<PackageName>SDL SDL_net SDL_image</PackageName>
			<FileCheck>/usr/pkg/include/SDL/SDL.h, /usr/pkg/include/SDL/SDL_net.h, /usr/pkg/include/SDL/SDL_image.h</FileCheck>
		</Package>
		<Package>
			<GenericName>glut</GenericName>
			<PackageName>glut</PackageName>
			<FileCheck>/usr/pkg/lib/libglut.la, /usr/pkg/include/GL/glut.h</FileCheck>
		</Package>
		<Package>
			<GenericName>libpng-development</GenericName>
